{
    "name": "letsbook-docusaurus",
    "version": "0.0.0",
    "private": true,
    "type": "module",
    "scripts": {
        "docusaurus": "docusaurus",
        "prestart": "node scripts/generate-latest-release.js",
        "start": "docusaurus start",
        "prebuild": "node scripts/generate-latest-release.js",
        "build": "docusaurus build",
        "postbuild": "cp build/redocusaurus/api.yaml build/api.yaml && npm run generate-api-rss",
        "generate-api-rss": "node scripts/generate-openapi-rss.js",
        "swizzle": "docusaurus swizzle",
        "deploy": "docusaurus deploy",
        "clear": "docusaurus clear",
        "serve": "docusaurus serve",
        "write-translations": "docusaurus write-translations",
        "write-heading-ids": "docusaurus write-heading-ids",
        "typecheck": "tsc",
        "openapi-check": "redocly lint api/index.openapi.yaml",
        "prettier-check": "prettier --check api src docs scripts releases",
        "prettier": "prettier -w api src docs scripts releases .junie docusaurus.config.ts | grep -v \"(unchanged)\"",
        "find-unreferenced": "node scripts/find-unreferenced-files.js",
        "compress-videos": "node scripts/compress-videos.js",
        "compress-images": "node scripts/compress-images.js"
    },
    "dependencies": {
        "@docusaurus/core": "^3.9.2",
        "@docusaurus/plugin-client-redirects": "^3.9.2",
        "@docusaurus/preset-classic": "^3.9.2",
        "@docusaurus/theme-mermaid": "^3.9.2",
        "@mdx-js/react": "^3.0.0",
        "clsx": "^2.0.0",
        "dotenv": "^17.2.3",
        "lucide-react": "^0.545.0",
        "prism-react-renderer": "^2.3.0",
        "react": "^19.0.0",
        "react-dom": "^19.0.0",
        "redocusaurus": "^2.5.0"
    },
    "devDependencies": {
        "@docusaurus/module-type-aliases": "^3.9.2",
        "@docusaurus/tsconfig": "^3.9.2",
        "@docusaurus/types": "^3.9.2",
        "@redocly/cli": "^2.11.1",
        "@trivago/prettier-plugin-sort-imports": "^5.2.2",
        "playwright": "^1.55.0",
        "prettier": "^3.6.2",
        "sharp": "^0.34.4",
        "typescript": "~5.6.2"
    },
    "browserslist": {
        "production": [
            ">0.5%",
            "not dead",
            "not op_mini all"
        ],
        "development": [
            "last 3 chrome version",
            "last 3 firefox version",
            "last 5 safari version"
        ]
    },
    "engines": {
        "node": ">=18.0"
    }
}
